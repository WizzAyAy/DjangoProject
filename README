### CREER SON ENV ###

 # CREER SON ENV
 conda create --name myenv
 
 # ACTIVER SON ENV
 conda activate myenv

### A FAIRE AVANT DE LANCER LE SITE ###

# INSTAL TOUTES LES LIBRAIRIES
pip install -r requirements.txt

# FAIT LES MIGRATIONS
python manage.py migrate

# CREER UNE MIGRATION
python manage.py makemigrations
